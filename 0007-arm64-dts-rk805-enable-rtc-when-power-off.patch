+++ b/arch/arm64/boot/dts/rockchip/rk3328-roc-cc.dts
@@ -243,6 +243,10 @@
 		vcc5-supply = <&vcc_io>;
 		vcc6-supply = <&vcc_io>;
 
+		rtc {
+			status = "okay";
+		};
+
 		regulators {
 			vdd_logic: DCDC_REG1 {
 				regulator-name = "vdd_logic";
